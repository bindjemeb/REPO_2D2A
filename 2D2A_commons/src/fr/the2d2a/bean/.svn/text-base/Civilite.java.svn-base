package fr.the2d2a.bean;

import java.io.Serializable;

public class Civilite implements Serializable {

	private static final long serialVersionUID = -2650860963402562774L;
	
	private int civId;
	private String titre;
	
	public int getCivId() {
		return civId;
	}
	public void setCivId(int id) {
		this.civId = id;
	}
	public String getTitre() {
		return titre;
	}
	public void setTitre(String titre) {
		this.titre = titre;
	}
}