package fr.the2d2a.bean;

import java.io.Serializable;

import org.springframework.web.multipart.MultipartFile;

public class AccueilElement implements Serializable {

	private static final long serialVersionUID = -6265604423548827573L;
	
	
	private String id;
	private int accId;
	private String titreElement;
	private String texteElement;
	private String imageElement;
	private int position;
	
	//pour le BO
	private MultipartFile imageElementFile;
	private String lang;
	
	public AccueilElement() {}
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getTitreElement() {
		return titreElement;
	}
	public void setTitreElement(String titreElement) {
		this.titreElement = titreElement;
	}
	public String getTexteElement() {
		return texteElement;
	}
	public void setTexteElement(String texteElement) {
		this.texteElement = texteElement;
	}
	public String getImageElement() {
		return imageElement;
	}
	public void setImageElement(String imageElement) {
		this.imageElement = imageElement;
	}
	public int getPosition() {
		return position;
	}
	public void setPosition(int position) {
		this.position = position;
	}
	public int getAccId() {
		return accId;
	}
	public void setAccId(int accId) {
		this.accId = accId;
	}

	public MultipartFile getImageElementFile() {
		return imageElementFile;
	}

	public void setImageElementFile(MultipartFile imageElementFile) {
		this.imageElementFile = imageElementFile;
	}

	public String getLang() {
		return lang;
	}

	public void setLang(String lang) {
		this.lang = lang;
	}
	
}
