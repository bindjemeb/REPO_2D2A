package fr.the2d2a.beanform;

import java.io.Serializable;

public class ArchivesForm implements Serializable {

	private static final long serialVersionUID = -6617659620940582338L;

	private String selectedLetter;
	private String selectedArtisteId;
	private String typeArtiste;
	
	public String getSelectedLetter() {
		return selectedLetter;
	}
	public void setSelectedLetter(String selectedLetter) {
		this.selectedLetter = selectedLetter;
	}
	public String getSelectedArtisteId() {
		return selectedArtisteId;
	}
	public void setSelectedArtisteId(String selectedArtisteId) {
		this.selectedArtisteId = selectedArtisteId;
	}
	public String getTypeArtiste() {
		return typeArtiste;
	}
	public void setTypeArtiste(String typeArtiste) {
		this.typeArtiste = typeArtiste;
	}
}
