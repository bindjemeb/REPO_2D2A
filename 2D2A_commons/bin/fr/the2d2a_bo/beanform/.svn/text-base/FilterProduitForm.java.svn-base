package fr.the2d2a_bo.beanform;

import java.io.Serializable;

public class FilterProduitForm implements Serializable {

	private static final long serialVersionUID = 8823692388629833957L;
	
	private int achatDepot = -1;
	private int visuLang = -1;
	private int rubrique = -1;
	private String reference;
	

	public int getAchatDepot() {
		return achatDepot;
	}

	public void setAchatDepot(int achatDepot) {
		this.achatDepot = achatDepot;
	}

	public int getVisuLang() {
		return visuLang;
	}

	public void setVisuLang(int visuLang) {
		this.visuLang = visuLang;
	}

	public int getRubrique() {
		return rubrique;
	}

	public void setRubrique(int rubrique) {
		this.rubrique = rubrique;
	}

	public String getReference() {
		return reference;
	}

	public void setReference(String reference) {
		this.reference = reference;
	}
	
	

}
