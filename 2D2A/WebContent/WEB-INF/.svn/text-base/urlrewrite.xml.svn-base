<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite>
 
    <rule>
        <from>/([a-z]+)/accueil.htm</from>
        <to>/accueil.htm?accFriendly=$1</to>
    </rule>

	<rule>
        <from>/([a-z]+)/produit.htm</from>
        <to>/produit.htm?produitFriendlyName=$1</to>
    </rule>

	<rule>
        <from>/([a-z_]+)/artiste.htm</from>
        <to>/artiste.htm?artiste=$1</to>
    </rule>
    
    <rule>
        <from>/quiSommesNous.htm</from>
        <to>/quiSommesNous.htm?idPage=3</to>
    </rule>
    
    <rule>
        <from>/contact.htm</from>
        <to>/contact.htm?idPage=4</to>
    </rule>
    
    <rule>
        <from>/infosLegales.htm</from>
        <to>/infosLegales.htm?idPage=5</to>
    </rule>
    
    <rule>
        <from>/serviceClient.htm</from>
        <to>/serviceClient.htm?idPage=6</to>
    </rule>

	<rule>
        <from>/aide.htm</from>
        <to>/aide.htm?idPage=7</to>
    </rule>

</urlrewrite>

